{
    "name": "pantheon-upstreams/upstream-configuration",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "pantheon-systems/drupal-integrations": "^9",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11 || ^12",
        "drupal/guardr_core": "^2.0",
        "mediacurrent/mis_profile": "^2.0",
        "mediacurrent/rain_gin": "^1.0",
        "mediacurrent/rain_core": "^5.0",
        "mediacurrent/rain_demo": "^5.0",
        "mediacurrent/rain_features": "^5.0",
        "mediacurrent/rain_theme": "^7.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "webflo/drupal-finder": "^1.0.0",
        "drupal/redis": "^1.6",
        "drupal/pantheon_advanced_page_cache": "^2.1"
    },
    "require-dev": {
        "drupal/core-dev": "^9.4",
        "drupal/drupal-extension": "^4.1",
        "drush-ops/behat-drush-endpoint": "^9.4.1",
        "mediacurrent/ci-scripts": "^2.0.0",
        "mediacurrent/ci-tests": "dev-master",
        "mglaman/drupal-check": "^1.0",
        "phpspec/prophecy-phpunit": "^2"
    },
    "extra": {
        "_README": "To make a custom upstream, clone this repository and add any dependencies to be provided by this upstream to this composer.json file. Leave the root-level composer.json file for the exclusive use of each individual site; do not modify it after your custom upstream has been published. See https://pantheon.io/docs/create-custom-upstream for more information."
    }
}
